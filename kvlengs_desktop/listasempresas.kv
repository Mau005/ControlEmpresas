<VListasEmpresas>:
    contenedor:contenedor
    MDBoxLayout:
        orientation: "horizontal"
        MDNavigationRail:
            anchor: "center"
            size_hint_x: .1
            MDNavigationRailItem:
                text: "Lista Empresas"
                icon: "arrow-down-bold-hexagon-outline"
                font_name: "nasalization-rg.ttf"

            MDNavigationRailItem:
                text: "Limpiar"
                icon: "autorenew"
                font_name: "nasalization-rg.ttf"
                on_release: root.formatear()


            MDNavigationRailItem:
                text: "Volver"
                icon: "keyboard-return"
                font_name: "nasalization-rg.ttf"
                on_release: root.siguiente()
        MDBoxLayout:
            orientation: "vertical"
            spacing: dp(20)
            padding: dp(10)
            MDCardPre:
                size_hint_x: .9
                pos_hint: {"center_x": .5, "center_y": .5}
                ScrollView:
                    do_scroll_y: True
                    do_scroll_x: False

                    MDList:
                        id: contenedor
                        padding: dp(30)