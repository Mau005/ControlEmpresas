

<VListaNotasEmpresas>:
    contenedor_registros:contenedor_registros
    MDBoxLayout:
        orientation: "horizontal"
        MDNavigationRail:
            anchor: "center"
            size_hint_x: .1
            elevation: .4
            MDNavigationRailItem:
                text: "Lista Notas Empresas"
                icon: "arrow-down-bold-hexagon-outline"
                font_name: "nasalization-rg.ttf"

            MDNavigationRailItem:
                text: "Buscar"
                icon: "layers-search-outline"
                font_name: "nasalization-rg.ttf"
                on_release: root.crear()

            MDNavigationRailItem:
                text: "Limpiar"
                icon: "autorenew"
                font_name: "nasalization-rg.ttf"
                on_release: root.formatear()


            MDNavigationRailItem:
                text: "Volver"
                icon: "keyboard-return"
                font_name: "nasalization-rg.ttf"
                on_release: root.siguiente()
        MDBoxLayout:
            orientation: "vertical"
            spacing: dp(20)
            padding: dp(10)
            MDCardPre:
                orientation: "vertical"
                pos_hint: {"center_x": .5, "center_y": .5}
                padding: dp(15)
                MDBoxLayout:
                    size_hint_y: None
                    height: dp(35)
                    MDLabel:
                        text: "Buscar: "
                    MDRoundFlatButton:
                        text: "Rut Empresa"
                        id: rut_empresa
                ScrollView:
                    do_scroll_x: False
                    do_scroll_y: True
                    MDList:
                        id: contenedor_registros
                        spacing: dp(15)
                        padding: dp(20)
