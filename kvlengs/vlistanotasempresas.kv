

<VListaNotasEmpresas>:
    contenedor_registros:contenedor_registros
    MDCard:
        size_hint_y: .15
        elevation: 5
        pos_hint: {"center_x": .5, "center_y": .5}

        MDLabel:
            text: "Buscar: "
        MDRectangleFlatButton:
            text: "Rut Empresa"
            id: rut_empresa
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        MDList:
            id: contenedor_registros
            spacing: dp(15)
            padding: dp(20)

    MDFloatingActionButtonSpeedDial:
        on_release_stack_button: root.accion_boton(*args)
        id: botones_notas_empresas
        root_button_anim: True