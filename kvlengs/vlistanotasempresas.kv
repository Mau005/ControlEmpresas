

<VListaNotasEmpresas>:
    contenedor_registros:contenedor_registros

    MDBoxLayout:
        orientation: "vertical"
        padding: dp(10)
        spacing: dp(15)
        MDCardPre:
            size_hint_y: .15
            elevation: 30
            pos_hint: {"center_x": .5, "center_y": .5}
            orientation: "horizontal"
            padding: dp(10)

            MDLabel:
                text: "Buscar: "
            MDRectangleFlatButton:
                text: "Rut Empresa"
                id: rut_empresa
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            MDList:
                id: contenedor_registros
                spacing: dp(15)
                padding: dp(20)

        MDFloatingActionButtonSpeedDial:
            callback: root.accion_boton
            size_hint_y: .1
            id: botones
            root_button_anim: True