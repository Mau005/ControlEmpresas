

<VNotasEmpresas>:
    botones:botones
    MDCardPre:
        orientation: 'vertical'
        elevation: 10
        spacing: "20dp"
        pos_hint: {"center_x": .5, "center_y": .5}
        size_hint: .95,.95
        padding: dp(45)

        MDList:
            cols: 2
            size_hint_y: .7

            MDTextField:
                hint_text: "ID Notas"
                disabled: True

            MDTextField:
                hint_text: "Rut Empresa"
                disable: True

            MDTextField:
                hint_text: "Correo"

            MDTextField:
                hint_text: "Fecha Creacion"
                disabled: True

            MDTextField:
                size_hint_y : None
                hint_text: "Nota"
                height: dp(300)
                multiline: True

            
    
    MDFloatingActionButtonSpeedDial:
        callback: root.accion_boton
        id: botones
        size_hint_y: .1
        root_button_anim: True
