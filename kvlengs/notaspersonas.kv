<VNotasPersonas>:
    MDCard:
        elevation: 5
        pos_hint: {"center_x": .5, "center_y": .5}
        size_hint: .9,.9
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            MDList:
                spacing: dp(15)
                padding: dp(45)
                MDRaisedButton:
                    text: "Rut Persona:"
                    id: boton_persona

                MDTextField:
                    id: nota_persona
                    size_hint_y : None
                    hint_text: "Nota"
                    height: dp(300)
                    multiline: True

            
    
    MDFloatingActionButtonSpeedDial:
        on_release_stack_button: root.accion_boton(*arg)
        id: botones_notas_personas
        root_button_anim: True
